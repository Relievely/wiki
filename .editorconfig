root = true

[*]
charset = utf-8
end_of_line = lf
indent_style = space
insert_final_newline = false
trim_trailing_whitespace = true
max_line_length = 160
tab_width = 4

[{*.har,*.jsb2,*.jsb3,*.json,.babelrc,.eslintrc,.prettierrc,.stylelintrc,bowerrc,jest.config}]
indent_size = 2

[{*.yaml,*.yml}]
indent_size = 2